<h1>New Post</h1>
<%= form_for @post, url: create_post_path, html: { multipart: true } do |f| %>

  <%= f.label :title, "Post Title" %>
  <%= f.text_field :title %><br>

  <%= f.label :description, "Post Description" %>
  <%= f.text_field :description %><br>

  <%= f.fields_for :images do |b| %>
    <%= b.label :data, "Image File" %>
    <%= b.file_field :data %><br>
  <% end %>

  <%= f.submit "Post" %>

<% end %>